int main() {
    int a = 2;
    int b = 3;

    int r1 = a < b;   // 1
    int r2 = a > b;   // 0
    int r3 = a <= b;  // 1
    int r4 = a >= b;  // 0
    int r5 = a == b;  // 0
    int r6 = a != b;  // 1

    int r7 = r1 && r3;      // 1 && 1 → 1
    int r8 = r2 || r4;      // 0 || 0 → 0
    int r9 = !r5;           // !0 → 1
    int r10 = !r7;          // !1 → 0

    return r1 + r2 + r3 + r4 + r5 + r6 + r7 + r8 + r9 + r10;
}
